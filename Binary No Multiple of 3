#include<bits/stdc++.h>
using namespace std;

bool multiple(char c[], int size){
	char state = '0';
	for(char i=0; i<size; i++){
		char digit = c[i];
		switch(state){
			case '0':
				if(digit == '1')
					state = '1';
				break;
			case '1':
				if(digit == '1')
					state = '0';
				else
					state = '2';
				break;
			case '2':
				if(digit == '0')
					state = '1';
				break;
		}
	}
	if(state == '0') return true;
	return false;
}


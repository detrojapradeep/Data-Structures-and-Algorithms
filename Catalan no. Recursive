#include<bits/stdc++.h>
using namespace std;

//// Recurcive approach!
unsigned long int catalan(unsigned int n){
	
	// catalan(n) is sum of catalan(i)*catalan(n-i-1)
	// 0,1,2,3..... = 1, 1, 2, 5, 14, 42, 132, 429, 1430, 4862, â€¦
	if(n <= 1) return 1;
	unsigned long int res = 0;
	for(int i=0; i<n; i++){
		res += catalan(i)*catalan(n-i-1);
	}
	return res;
	
}

int main(){
	int n;
	cin>>n;
	cout<<catalan(n);
	return 0;
}
